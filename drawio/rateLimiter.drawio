        <mxCell id="9l1T_0m_hgeEZGjoY-rE-15" value="this kind of design is helpful in smoothening out sudden burst of traffic" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="1">
